@model IEnumerable<DesarrolloTallerMecanico.Models.Data.DDL.ServicioMecanico>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaIngresoVehiculo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaEntregaVehiculo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DescripcionServicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vehiculo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoServicio)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaIngresoVehiculo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaEntregaVehiculo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DescripcionServicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vehiculo.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoServicio.Nombre)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
